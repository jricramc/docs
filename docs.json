{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nerd - AI Software Builder",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Launch Nerd",
          "href": "https://nerdapp.ai",
          "icon": "rocket"
        }
      ]
    },
    "languages": [
      {
        "language": "en",
        "groups": [
          {
            "group": "Welcome",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Learn",
            "pages": [
              "how-to-prompt",
              "examples"
            ]
          }
        ]
      },
      {
        "language": "es",
        "groups": [
          {
            "group": "Bienvenido",
            "pages": [
              "es/index",
              "es/quickstart"
            ]
          },
          {
            "group": "Aprende",
            "pages": [
              "es/how-to-prompt",
              "es/examples"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/lentes.svg",
    "dark": "/logo/lentes.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Nerd",
      "href": "https://nerdapp.ai"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "cursor"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/nerdapp"
    }
  }
}
